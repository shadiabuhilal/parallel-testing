{
  "name": "parallel-testing",
  "version": "1.0.1",
  "description": "parallel-testing allows the execution of any test frameworks in parallel, dramatically reducing the execution time.",
  "main": "src/index.js",
  "bin": {
    "parallel-testing": "./src/index.js"
  },
  "scripts": {
    "lint": "eslint **/*.spec.js --fix",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "commit": "git-cz"
  },
  "dependencies": {
    "arg": "5.0.2",
    "chalk": "2.4.2",
    "concurrently": "8.2.2",
    "fast-glob": "3.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/release-notes-generator": "14.0.0",
    "@stylistic/eslint-plugin-js": "2.1.0",
    "@types/chalk": "2.2.0",
    "@types/jest": "29.5.12",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.4.0",
    "eslint-plugin-jest": "28.6.0",
    "jest": "29.7.0",
    "jest-html-reporters": "3.1.7",
    "semantic-release": "24.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "parallel",
    "test",
    "testing",
    "e2e",
    "cy",
    "cypress",
    "protractor",
    "puppeteer",
    "webdriverIO",
    "webdriver",
    "selenium",
    "Nightwatch",
    "report",
    "mochawesome",
    "json",
    "html",
    "merge"
  ],
  "author": "Shadi Abu Hilal <shadiabuhilal@gmail.com> (http://github.com/shadiabuhilal)",
  "license": "MIT",
  "homepage": "https://github.com/shadiabuhilal/parallel-testing",
  "bugs": {
    "url": "https://github.com/shadiabuhilal/parallel-testing/issues"
  }
}
